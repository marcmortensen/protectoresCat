{
  "name": "@protectorescat/shelters",
  "version": "0.0.0",
  "description": "Node.js development in TypeScript that scrappes animal shelter websites",
  "type": "module",
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "~9.10",
    "@types/eslint__js": "~8.42",
    "@types/node": "^20.17.10",
    "rimraf": "~6.0",
    "ts-api-utils": "~1.3",
    "typescript": "~5.5",
    "vitest": "~2.1",
    "@vitest/coverage-v8": "~2.1"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "move": "mv result_*.json ../../apps/web/content/",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "test:unit-run": "vitest run unit",
    "test:coverage": "vitest run unit --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:unit-watch": "vitest unit --watch"
  },
  "author": "Marc Mortensen",
  "license": "Apache-2.0",
  "dependencies": {
    "jsdom": "^25.0.1",
    "tslib": "~2.7"
  },
  "volta": {
    "node": "20.17.0"
  },
  "main": "index.js",
  "keywords": []
}
